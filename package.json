{
  "name": "starwars",
  "version": "2020.3.0",
  "description": "StarWars API Demo",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.dev.js --env.NODE_ENV=dev",
    "prdbuild": "webpack --config webpack.prod.js --env.NODE_ENV=prod",
    "test": "jest",
    "start": "webpack-dev-server --config webpack.dev.js --env.NODE_ENV=dev --open"
  },
  "author": "Star Wars Team",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "axios": "^0.18.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-react-intl": "^2.0.3",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^5.16.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.19.0",
    "fetch-mock": "^7.7.3",
    "file-loader": "^3.0.1",
    "history": "^4.9.0",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.12.2",
    "jest": "^24.7.1",
    "jest-environment-jsdom": "^24.7.1",
    "jest-environment-jsdom-global": "^1.2.1",
    "jest-junit": "^6.3.0",
    "joi-browser": "^13.4.0",
    "lodash": "^4.17.20",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.19.3",
    "npm": "^6.14.8",
    "radium": "^0.25.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-joi-validation": "^1.7.0",
    "react-router-dom": "^5.1.1",
    "react-slidedown": "^2.4.5",
    "react-test-renderer": "^16.13.0",
    "redux-form": "^8.3.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "sinon": "^7.5.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "dotenv": "^6.2.0",
    "downloadjs": "^1.4.7",
    "markdown-to-jsx": "^6.11.4",
    "moment": "^2.24.0",
    "prop-types": "^15.6.2",
    "react-calendar": "^2.19.1",
    "react-ga": "^2.5.7",
    "react-google-recaptcha": "^2.1.0",
    "react-intl": "^2.8.0",
    "react-player": "^2.5.0",
    "react-redux": "^7.2.0",
    "react-responsive-carousel": "^3.2.9",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "stringquery": "^1.0.8"
  },
  "jest": {
    "testEnvironment": "jest-environment-jsdom-global",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/__test__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setupTests.js"
    ],
    "setupFiles": [
      "raf/polyfill"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/src/test/assetsTransformer.js",
      "^app(.*)$": "<rootDir>/src$1"
    },
    "collectCoverage": true,
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageDirectory": "package/coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/store/configureStore.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/precompiled/",
      "/src/test"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "src/test"
    ],
    "coverageThreshold": {
      "global": {
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "package/coverage"
  }
}
